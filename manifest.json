{
    "manifest_version": 2,
    "content_scripts": [{
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "chokato.user.js" ],
        "matches":          [   "http://www.neopets.com/*",
                                "http://www.neopets.com/*"
                            ]
    }],
